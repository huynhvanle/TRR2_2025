#include <bits/stdc++.h>
#define all(x) x.begin(), x.end()

int main() {
    freopen("DT.INP", "r", stdin);
    freopen("DT.OUT", "w", stdout);
    int t, n;
    cin >> t >> n;
    int mtk[101][101];

    for(int i = 1; i <= n; i++)
        for(int j = 1; j <= n; j++){
            cin >> mtk[i][j];
        }
    if(t == 1){
        for(int i = 1; i <= n; i++){
            int deg1 = 0, deg2 = 0;
            for(int j = 1; j <= n; j++){
                if(mtk[i][j]) deg1++;
                if(mtk[j][i]) deg2++;
            }
            cout << deg2 << " " << deg1 << endl;
        }
    }else if(t == 2){
        cout << n << endl;
        for(int i = 1; i <= n; i++){
            vector<int> dsk;
            for(int j = 1; j <= n; j++){
                if(mtk[i][j])
                    dsk.push_back(j);
            }
            cout << dsk.size() << " ";
            for(int x : dsk)
                cout << x << " ";
            cout << endl;
        }
    }
    return 0;
}
