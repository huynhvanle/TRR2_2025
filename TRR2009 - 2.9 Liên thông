#include <bits/stdc++.h>
#define all(x) x.begin(), x.end()

int n;
int a[101][101];
int check[101];

vector<set<int>> rs;

void dfs(int u){
    set<int> tmp;
    stack<int> s;
    s.push(u);
    tmp.insert(u);
    check[u] = 1;
    while(!s.empty()){
        int i = s.top();
        s.pop();
        for(int j = 1; j <= n; j++){
            if(!check[j] && a[i][j]){
                check[j] = 1;
                tmp.insert(j);
                s.push(i);
                s.push(j);
                break;
            }
        }
    }
    rs.push_back(tmp);
}

void find(){
    for(int i = 1; i <+ n; i++){
        if(!check[i]){
            dfs(i);
        }
    }
    cout << rs.size() << endl;
    for(set<int> x : rs){
        for(int y : x)
            cout << y << " ";
        cout << endl;
    }
}

int main() {
    freopen("TK.INP", "r", stdin);
    freopen("TK.OUT", "w", stdout);
    cin >> n;
    for(int i = 1; i <= n; i++)
        for(int j = 1; j <= n; j++)
            cin >> a[i][j];
    find();
    return 0;
}
