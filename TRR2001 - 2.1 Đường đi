#include <bits/stdc++.h>
#define all(x) x.begin(), x.end()

int mtk[101][101];
int t, n, u, v;
int check[101], truoc[101];

void find_1(){
    int tmp[101][101];
    for(int i = 1; i <= n; i++){
        for(int j = 1; j <= n; j++){
            tmp[i][j] = 0;
            for(int k = 1; k <= n; k++)
                tmp[i][j] += mtk[i][k] * mtk[k][j];
        }
    }
    cout << tmp[u][v];
}

void find_2(){
    stack<int> s;
    s.push(u);
    check[u] = 1;
    while(!s.empty()){
        int i = s.top();
        s.pop();
        for(int j = 1; j <= n ; j++){
            if(mtk[i][j] && !check[j]){
                check[j] = 1;
                truoc[j] = i;
                s.push(i);
                s.push(j);
                break;
            }
        }
    }
    if(truoc[v]){
        vector<int> dd;
        while(v){
            dd.push_back(v);
            v = truoc[v];
        }
        reverse(all(dd));
        for(int x : dd)
            cout << x << " ";
    }else
        cout << 0;
}

int main() {
    freopen("TK.INP", "r", stdin);
    freopen("TK.OUT", "w", stdout);
    cin >> t >> n >> u >> v;
    for(int i = 1; i <= n; i++)
        for(int j = 1; j <= n; j++)
            cin >> mtk[i][j];
    
    if(t == 1)
        find_1();
    else if(t == 2){
        find_2();
    }
    return 0;
}
