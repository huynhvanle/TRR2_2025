#include <bits/stdc++.h>
#define all(x) x.begin(), x.end()

int mtk[101][101];
int n, u;
int x[101], check[101];
vector<vector<int>> ham;

void find_ham(int i){
    for(int j = 1; j <= n; j++){
        if(mtk[x[i - 1]][j]){
            if(!check[j]){
                x[i] = j;
                check[j] = 1;
                find_ham(i + 1);
                check[j] = 0;
            }
            if(i == n + 1 && j == x[1]){
                vector<int> kq;
                for(int m = 1; m <= n; m++)
                    kq.push_back(x[m]);
                kq.push_back(j);
                ham.push_back(kq);
            }
        }
    }
}
int main() {
    cin >> n >> u;
    for(int i = 1; i <= n; i++)
        for(int j = 1; j <= n; j++)
            cin >> mtk[i][j];
    x[1] = u;
    check[u] = 1;
    find_ham(2);
    if(ham.empty()) cout << 0;
    else{
        cout << ham.size() << endl;
        for(vector<int> x : ham){
            for(int y : x)
                cout << y << " ";
            cout << endl;
        }
    }
    return 0;
}
